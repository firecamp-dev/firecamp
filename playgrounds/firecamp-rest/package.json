{
  "name": "@firecamp/rest",
  "version": "0.0.0",
  "description": "Rest Playground, Firecamp",
  "main": "src/index.ts",
  "license": "AGPLv3",
  "private": true,
  "keywords": [],
  "type": "module",
  "scripts": {
    "build": "pnpm clean && run-p 'build:*' && pnpm copy-files",
    "build:main": "tsc -p tsconfig.json",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 \"src/**/*.sass\" \"src/**/*.css\" dist/",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src --ext .ts --fix",
    "test": "npx jest --bail --silent --verbose",
    "test:lint": "eslint src --ext .ts",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:spelling": "cspell \"{README.md,.github/*.md,src/**/*.ts}\"",
    "test:code-snippet": "jest packages-clients/rest/src/services/code-snippet",
    "check-cli": "run-s test diff-integration-tests check-integration-tests",
    "check-integration-tests": "run-s check-integration-test:*",
    "diff-integration-tests": "mkdir -p diff && rm -rf diff/test && cp -r test diff/test && rm -rf diff/test/test-*/.git && cd diff && git init --quiet && git add -A && git commit --quiet --no-verify --allow-empty -m 'WIP' && echo '\\n\\nCommitted most recent integration test output in the \"diff\" directory. Review the changes with \"cd diff && git diff HEAD\" or your preferred git diff viewer.'",
    "watch:build": "tsc -p tsconfig.json -w",
    "doc": "run-s doc:html && open-cli build/docs/index.html",
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && npm i",
    "prepare-release": "run-s reset-hard test cov:check doc:html version doc:publish"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@firecamp/curl-to-firecamp": "^1.0.2",
    "@firecamp/scripts": "workspace:*",
    "@firecamp/types": "^0.2.2",
    "@firecamp/ui": "workspace:*",
    "@firecamp/url": "0.0.20",
    "@firecamp/utils": "^0.0.24",
    "@hapi/joi": "^17.1.1",
    "aws4": "^1.11.0",
    "chai-http": "^4.3.0",
    "classnames": "^2.1.3",
    "clean-deep": "3.4.0",
    "codemirror": "^5.63.0",
    "codemirror-graphql": "^1.0.2",
    "comment-json": "^4.1.1",
    "crypto-js": "^4.1.1",
    "debounce-promise": "^3.1.2",
    "fuzzysort": "^1.1.4",
    "graphql": "^15.6.0",
    "hawk": "^9.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "nanoid": "3.3.4",
    "prop-types": "^15.6.2",
    "re-resizable": "^4.7.1",
    "react-file-drop": "^3.1.5",
    "react-ga": "^3.1.2",
    "react-hotkeys-hook": "^3.3.1",
    "tv4": "^1.3.0",
    "xml-js": "^1.6.9"
  },
  "devDependencies": {
    "@types/chrome": "0.0.204",
    "@types/har-format": "^1.2.8",
    "@types/httpsnippet": "^1.23.1",
    "@types/jest": "29.2.4",
    "@types/lodash": "^4.14.175",
    "@types/md5": "^2.3.2",
    "@types/node": "^16.4.3",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.29.0",
    "cspell": "^4.1.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-import": "^2.22.0",
    "httpsnippet": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "open-cli": "^6.0.1",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.8",
    "typescript": "^5.0.2"
  },
  "resolutions": {
    "codemirror-graphql/graphql": "^15.6.0"
  },
  "files": [
    "build/main",
    "build/module",
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "prettier": {
    "singleQuote": true
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ]
  }
}
